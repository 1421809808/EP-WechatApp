}

.pos-f {
  position: fixed;
}

.avatar {
  width: 80rpx;
  height: 80rpx;
  border-radius: 50%;
}

.shadow {
  box-shadow: 1px 2px 5px #ccc;
}

.shadow-top {
  box-shadow: 0px -3px 5px -2px #ccc;
}

.avatar-md {
  width: 100rpx;
  height: 100rpx;
  border-radius: 50%;
  overflow: hidden;
}

