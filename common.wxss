}

.border-bottom,
.border-top {
  position: relative;
}

.border-bottom::after {
  content: "";
  position: absolute;
  left: 0;
  bottom: 0;
  right: 0;
  border-bottom: 1px solid #e5e5e5;
  transform-origin: 0 0;
  transform: scaleY(0.5);
}

.border-top::before {
  content: "";
  position: absolute;
  left: 0;
  top: 0;
  right: 0;
  border-bottom: 1px solid #e5e5e5;
  transform-origin: 0 0;
  transform: scaleY(0.5);
}

.btn-hollow {
  border: 1rpx solid #e5e5e5;
  padding: 12rpx 20rpx;
  border-radius: 30rpx;
  font-size: 22rpx;
  color: #666;
  line-height: 1;
}

.line-through {
  text-decoration: line-through;
}

.line-height {
  line-height: 1.5;
}

/* 定位 */
.pos-r {
